{
  "name": "@workspace/api",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./router/*": {
      "types": "./dist/router/*.d.ts",
      "default": "./src/router/*.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@effect/sql-drizzle": "^0.45.0",
    "@workspace/database": "workspace:*",
    "drizzle-orm": "^0.44.6",
    "effect": "^3.17.14"
  },
  "devDependencies": {
    "@types/node": "^20.19.9",
    "typescript": "^5.7.3"
  }
}

