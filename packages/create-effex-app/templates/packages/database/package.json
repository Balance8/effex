{
  "name": "@workspace/database",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "db:generate": "prisma generate && bun scripts/fix-updated-at.ts && bun scripts/generate-effect-schemas.ts",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "bun scripts/seed.ts"
  },
  "prisma": {
    "seed": "bun scripts/seed.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@prisma/generator-helper": "^6.2.1",
    "@prisma/internals": "^6.2.1",
    "@types/node": "^24.9.1",
    "@types/pg": "^8.11.10",
    "drizzle-prisma-generator": "^0.1.7",
    "prisma": "^6.2.1",
    "tsx": "^4.19.2"
  },
  "dependencies": {
    "@effect/platform": "^0.91.1",
    "@effect/schema": "^0.75.5",
    "@effect/sql": "^0.45.0",
    "@effect/sql-drizzle": "^0.45.0",
    "@effect/sql-pg": "^0.47.0",
    "@handfish/drizzle-effect": "^0.0.17",
    "@prisma/client": "^6.2.1",
    "drizzle-orm": "^0.44.6",
    "effect": "^3.17.14",
    "pg": "^8.13.1"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./drizzle/client": {
      "types": "./src/drizzle-client.ts",
      "default": "./src/drizzle-client.ts"
    },
    "./effect/client": {
      "types": "./src/effect-client.ts",
      "default": "./src/effect-client.ts"
    },
    "./drizzle/schema": {
      "types": "./src/generated/drizzle/schema.ts",
      "default": "./src/generated/drizzle/schema.ts"
    },
    "./effect/*": {
      "types": "./src/generated/effect/*.ts",
      "default": "./src/generated/effect/*.ts"
    }
  }
}
