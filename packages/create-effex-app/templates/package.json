{
  "name": "{{projectName}}",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web",
    "dev:expo": "turbo run dev --filter=expo",
    "format-and-lint": "biome check .",
    "format-and-lint:fix": "biome check . --write --unsafe",
    "db:generate": "turbo run db:generate",
    "db:migrate": "turbo run db:migrate",
    "db:seed": "turbo run db:seed",
    "db:studio": "turbo run db:studio",
    "effect:generate": "cd packages/database && {{packageManagerExecutable}} effex-gen@latest gen",
    "clean": "git clean -fdx -e '.env*' && {{packageManagerInstall}}",
    "{{huskyPrepareScript}}"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@workspace/typescript-config": "workspace:*",
    "husky": "^9.1.7",
    "turbo": "^2.5.8",
    "typescript": "^5.9.2",
    "ultracite": "^6.0.4"
  },
  "packageManager": "{{packageManager}}",
  "engines": {
    "node": ">=20"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
